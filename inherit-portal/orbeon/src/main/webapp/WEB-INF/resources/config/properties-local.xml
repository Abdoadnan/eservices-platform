<!--
    Motrice.org Orbeon properties-local.xml file. Please follow the instructions here:
    http://wiki.orbeon.com/forms/doc/developer-guide/configuration-properties
-->
<properties xmlns:xs="http://www.w3.org/2001/XMLSchema"
            xmlns:oxf="http://www.orbeon.com/oxf/processors">
  
  <property as="xs:string" name="oxf.fr.default-language.*.*" value="sv"/>
  
   <property as="xs:string" name="oxf.fr.css.custom.uri.*.*" value="/config/malmo.css"/>

  <property
      as="xs:string"
      name="oxf.fr.persistence.provider.*.*.*"
      value="exist"/>
  
  <property 
      as="xs:anyURI" 
      name="oxf.fr.persistence.exist.uri" 
      value="/fr/service/exist"/>

  <property 
      as="xs:anyURI" 
      name="oxf.fr.persistence.exist.exist-uri" 
      value="http://orbeon:orb@localhost:8080/exist/rest/db/orbeon-pe/fr"/>

  <property as="xs:string" name="oxf.fr.detail.buttons.*.*" value="clear save-draft send"/>
  <property as="xs:string" name="oxf.fr.detail.buttons.anonymous.*" value="send"/>
  <property as="xs:string" name="oxf.fr.detail.buttons.start.demo-ansokan" value="send"/>
  <property as="xs:string" name="oxf.fr.detail.buttons.miljoforvaltningen.anmalan-hemkompostering" value="send"/>
  
  <property as="xs:string" name="oxf.fr.detail.buttons.malmo.*" value="clear save-final"/>

  <property as="xs:string" name="oxf.fr.detail.buttons.view.*.*" value=""/>
  <property as="xs:string" name="oxf.fr.detail.submit.buttons.*.*" value="go"/>

  <property as="xs:anyURI" name="oxf.fr.detail.send.success.uri.*.*"
          value="https://eservices.malmo.se/site/mycases/form/confirmdispatcher"/>
  <property as="xs:string" name="oxf.fr.detail.send.success.method.*.*" value="get"/>


  <!-- HTTP method to use to navigate to the error page -->
  <property as="xs:string" name="oxf.fr.detail.send.error.method.*.*" value="get"/>
  
  <property as="xs:string" name="oxf.fr.resource.*.*.sv.detail.buttons.send" value="Skicka"/>
  <property as="xs:string" name="oxf.fr.resource.*.*.sv.detail.buttons.clear" value="Rensa"/>
  <property as="xs:string" name="oxf.fr.resource.*.*.sv.detail.buttons.save-draft" value="Spara"/>
  <property as="xs:string" name="oxf.fr.resource.*.*.sv.detail.buttons.save-final" value="Spara"/>
  
  <property as="xs:string" name="oxf.fr.resource.*.*.en.detail.buttons.send" value="Skicka"/>
  <property as="xs:string" name="oxf.fr.resource.*.*.en.detail.buttons.clear" value="Rensa"/>
  <property as="xs:string" name="oxf.fr.resource.*.*.en.detail.buttons.save-draft" value="Spara"/>
  <property as="xs:string" name="oxf.fr.resource.*.*.en.detail.buttons.save-final" value="Spara"/>
  
  <property as="xs:boolean" name="oxf.fr.noscript-link.*.*" value="false"/>
</properties>
